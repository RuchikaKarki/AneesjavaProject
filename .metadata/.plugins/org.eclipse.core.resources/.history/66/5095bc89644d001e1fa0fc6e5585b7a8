package JavaPractice;

public class JavaBasics {

	public static void main(String[] args) {
		
		/* boolean x = true;
		 double x = 45.58;
	 	x is a variable*/
		
		int x=100;
		System.out.println(x);
		
		char y ='A';
		System.out.println(y);
		
		double z =100.85;
		System.out.println("This is a value of double"+" "+z);
		
		String g = "Testing";
		System.out.println("This value is String "+g);  //Concatenation
		
		System.out.println(x +" "+ g);   //Concatenation
		
		String m="Automation Testing";
		String FirstName= "Ruchika";
		String LastName= "Karki";
		System.out.println(m+" "+FirstName+" "+LastName);
		
		
		
		
		
		
	}

}
